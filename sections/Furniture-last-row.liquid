
<div class="section-Furniture-last-row">
    <div class="wrapper">
    <h2>{{ section.settings.title }}</h2> 
     <p> {{ section.settings.title1 }}</p>
    
  

         <div class="main-Furniture-last-row">
                {% for block in section.blocks %}
                          <div class="flex">
                        <div class="images-Furniture-last-row">           
                        <img src="{{ block.settings.image | img_url: 'master' }}" >
                          </div>



                            
                        <div class="text-Furniture-last-row">  
                         <img src="{{ block.settings.imagess | img_url: 'master' }}" >
                         {{ block.settings.buy-text2 }} 
                       
                       </div> 
             
                
                   </div>
             {% endfor %}


  </div>
  </div>
 </div>


{% schema %}
  {
    "name": "Furniture-last-row",
    "settings": [
      {
        "type" : "text",
        "id" : "title",
        "label" : "Heading"
      },
       {
        "type" : "text",
        "id" : "title1",
        "label" : "Heading_two"
      }
        
    ],
    "blocks" : [
      {
        "type" : "image_picker",
        "name" : "Image",
        "settings": [
         {
            "type" : "image_picker",
            "id" : "image",
            "label" : "Image"
          },

          
           {
        "type" : "richtext",
        "id" : "buy-text2",
        "label" : "text"
      },

             {
            "type" : "image_picker",
            "id" : "imagess",
            "label" : "Image_2"
          }
        ]
      }
    ],
    "presets" : [
      {
        "name" : "Furniture-last-row"
      }
    ]
  }
{% endschema %}