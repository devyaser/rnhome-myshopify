{% schema %}
  {
    "name": "Team-section",
    "settings": [


      
               {
                "type":"image_picker",
                "id":"bg-img1",
                "label":"Image"
             },
     {
        "type" : "text",
        "id" : "title1",
        "label" : "Heading"
      },
      
           {
               "type":"richtext",
               "id":"buy-text1",
               "label":"last-Text"
            },
         {
      "id": "link",
      "type": "url",
      "label": "Button link"
    },

    {
      "id": "linktext",
      "type": "text",
      "label": "Button text",
      "default": "About Us"
    }
    ],
      "presets": [
    {
      "name": "Team-section"
    }
  ]
  }
{% endschema %}