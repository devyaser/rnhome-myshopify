

  <div class="team-section">
     <div class="wrapper">
       <div class="flex">
                  <div class="team-text">
              <h4>{{ section.settings.title1 }}</h5> 
            {{ section.settings.buy-text1 }} 
         <a href="{{ section.settings.link }}" class="button-our">{{ section.settings.linktext }}</a>
       </div>
     <div class="team-images">
      <img src="{{section.settings.bg-img1| img_url: 'master'}}" alt="">
